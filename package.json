{
  "name": "tk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check '**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx}'",
    "typecheck": "tsc",
